    "require": {
        "php": ">=5.4.0",
        "doctrine/common": "~2.2",
        "doctrine/dbal": "~2.2",
        "symfony/config": "~2.6|~3.0",
        "symfony/dependency-injection": "~2.6|~3.0",
        "symfony/doctrine-bridge": "~2.6|~3.0",
        "symfony/form": "~2.6|~3.0",
